   "source": [
    "import pandas as pd\n",
    "import random\n",
    "lst = ['robot'] * 10\n",
    "lst += ['human'] * 10\n",
    "random.shuffle(lst)\n",
    "data = pd.DataFrame({'whoAmI':lst})\n",
    "\n",
    "for value in lst:\n",
    "    data[value] = (data['whoAmI'] == value).astype(int)\n",
    "\n",
    "\n",
    "data.head()"
   ]
